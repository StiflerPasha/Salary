<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@android:color/white"/>
            <stroke android:width="2dp" android:color="#97938c" android:dashGap="3dp" android:dashWidth="5dp" />
            <corners
                android:radius="5dp"/>
            <padding
                android:bottom="2dp"
                android:top="2dp"
                android:left="5dp"
                android:right="5dp"/>
        </shape>
    </item>
</selector>